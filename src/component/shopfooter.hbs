<section>
  <div class='row'>
    {{#each footer}}
      <div class='col-2'>
        {{#each this}}
          <nav class='nav flex-column m-1'>
            {{#if @last}}<a
                class='nav-link disabled'
              >{{text}}</a>
            {{else}}
              <a
                class='nav-link active'
                aria-current='page'
                href='{{footer}}'
              >{{text}}</a>
            {{/if}}
          </nav>
        {{/each}}
      </div>
    {{/each}}
  </div>
</section>