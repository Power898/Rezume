<section>
  {{#with navigation}}
    <div class='row'>
      <div class='col-7'>
        <ul class='nav'>
          {{#each links}}
            <li class='nav-item'>
              <a
                class='nav-link'
                href='{{href}}'
              >{{text}}</a>
            </li>
          {{/each}}
          <li class='nav-item'>
            <a class='nav-link disabled'>Disabled</a>
          </li>
        </ul>
      </div>
    </div>
  {{/with}}
</section>
<hr class='mt-0 mb-4' />