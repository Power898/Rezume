{{! 
Це код container файлу для summary сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
}}

<div>
  <section>
      {{#with navigation}}
 <nav class="navbar navbar-expand-lg bg-body-tertiary">
  <div class="container-fluid">
    <a class="navbar-brand">{{Logo}}</a>
        <ul class='nav justify-content-end'>
          {{#each links}}
            <li class='nav-item'>
              <a
                class='nav-link'
                href='{{href}}'
              >{{text}}</a>
            </li>
          {{/each}}
          <li class='nav-item'>
            <a class='nav-link disabled'>Disabled</a>
          </li>
        </ul>
        </div>
      {{/with}}
  </section>

  <section>
    <div>
      {{#with newsBlock}}
        <h2 class='h2 text-center'>{{title}}</h2>
        <br />
        <div class='row'>
          {{#each cards}}
            <div class='col-3'>
              <div
                class='card text-bg-dark mb-3'
                style='max-width: 18rem;'
              >
                <div class='card-header'>{{title}}
                  {{#if isTop}}<span
                      class='badge text-bg-danger'
                    >Топ</span>{{/if}}
                  {{#unless isNew}}<span
                      class='badge text-bg-success'
                    >Нова</span>{{/unless}}
                </div>
                <div class='card-body'>
                  <p class='card-text'>{{description}}</p>
                </div>
              </div>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </div>
  </section>
<hr>
  <section>
    <div>
      <div class="row">
      {{#with goodsBlock}}
      <div class="col-12">
         {{#each tabs}}
         <ul class="nav nav-tabs">
         {{#if isActive}}
       <li class="nav-item">
     <a class="nav-link active" aria-current="page" href="#">{{text}}</a>
       </li>
        {{else}}
        <li class="nav-item">
        <a class="nav-link" href="#">{{text}}</a>
        </li>
         {{/if}}
         {{#if isDisabled}}
         <li class="nav-item">
    <a class="nav-link disabled">{{text}}</a>
         </li>
         {{/if}}
         {{/each}}
         </ul>
         </div>
         </div>
         <br>
         <div class="row gx-2 gy-2">
         {{#each cards}}
         <div class="col-4">
         <div class="card" style="width: 14rem;">
            <img src="{{image}}" class="card-img-top" alt="...">
           <div class="card-body">
             <h5 class="card-title">{{title}}
               {{#if isHot}} 
                  <span class="badge text-bg-warning">HOT!</span>
               {{/if}}
               {{#if isNew}}
                  <span class="badge text-bg-primary">New</span>
                {{/if}}
              </h5>
              <p class="card-text">{{description}}</p>
            </div>
         </div>
         </div>
         {{/each}}
         </div>
      {{/with}}
    </div>
  </section>
  <hr>
  <section>
    <div>
      {{#with subscribe}}
     <div class="card text-bg-dark">
  <div class="card-body">
    <h5 class="card-title">{{header}}</h5>
     <p class="text-end">
     <a href="{{buttons.[0].link}}" class="btn btn-primary">{{buttons.[0].text}}</a>
     <a href="{{buttons.[1].link}}" class="btn btn-primary">{{buttons.[1].text}}</a>
     </p>
    <p class="card-text">{{description}}</p>
  </div>
      {{/with}}
    </div>
  </section>
  <hr>
  <section>
    <div>
      {{#with service}}
      <div class="card text-center text-bg-success">
  <div class="card-body">
    <h5 class="card-title">{{title}}</h5>
    <p class="card-text">{{description}}</p>
    <a href="{{buttons.[0].link}}" class="btn btn-primary">{{buttons.[0].text}}</a>
  </div>
      {{/with}}
    </div>
  </section>
  <hr>
  <section>
  <div class="row">
    <div class="col-2">
      <nav class="nav flex-column">
       <a class="nav-link active" aria-current="page" href="{{footer.[0].[0].link}}">{{footer.[0].[0].text}}</a>
       <a class="nav-link" href="{{footer.[0].[1].link}}">{{footer.[0].[1].text}} </a>
       <a class="nav-link" href="{{footer.[0].[2].link}}">{{footer.[0].[2].text}}</a>
       <a class="nav-link disabled">{{footer.[0].[3].text}}</a>
      </nav>
    </div>
    <div class="col-2">
      <nav class="nav flex-column">
       <a class="nav-link active" aria-current="page" href="{{footer.[1].[0].link}}">{{footer.[1].[0].text}}</a>
       <a class="nav-link" href="{{footer.[1].[1].link}}">{{footer.[1].[1].text}}</a>
       <a class="nav-link" href="{{footer.[1].[2].link}}">{{footer.[1].[2].text}}</a>
       <a class="nav-link disabled">{{footer.[1].[3].text}}</a>
      </nav>
    </div>
    <div class="col-2">
      <nav class="nav flex-column">
       <a class="nav-link active" aria-current="page" href="{{footer.[2].[0].link}}">{{footer.[2].[0].text}}</a>
       <a class="nav-link" href="{{footer.[2].[1].link}}">{{footer.[2].[1].text}}</a>
       <a class="nav-link" href="{{footer.[2].[2].link}}">{{footer.[2].[2].text}}</a>
       <a class="nav-link disabled">{{footer.[2].[3].text}}</a>
      </nav>
    </div>
    <div class="col-2">
      <nav class="nav flex-column">
       <a class="nav-link active" aria-current="page" href="{{footer.[3].[0].link}}">{{footer.[3].[0].text}}</a>
       <a class="nav-link" href="{{footer.[3].[1].link}}">{{footer.[3].[1].text}}</a>
       <a class="nav-link" href="{{footer.[3].[2].link}}">{{footer.[3].[2].text}}</a>
       <a class="nav-link disabled">{{footer.[3].[3].text}}</a>
      </nav>
    </div>
    <div class="col-2">
      <nav class="nav flex-column">
       <a class="nav-link active" aria-current="page" href="{{footer.[4].[0].link}}">{{footer.[4].[0].text}}</a>
       <a class="nav-link" href="{{footer.[4].[1].link}}">{{footer.[4].[1].text}}</a>
       <a class="nav-link" href="{{footer.[4].[2].link}}">{{footer.[4].[2].text}}</a>
       <a class="nav-link disabled">{{footer.[4].[3].text}}</a>
      </nav>
    </div>
    </div>
  </section>
</div>

